<%= form_with model: @user do |form| %>
    <%= form.label :username, "username:" %>
    <%= form.text_field :username, value: "John"%>
    <%= form.label :email, "type your email:" %>
    <%= form.email_field :email, placeholder: "example@email.com"%>
    <%= form.label :password, "password:" %>
    <%= form.password_field :password %>
    <%= form.submit :Submit %>
  <% end %>


