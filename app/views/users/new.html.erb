<%= form_with model: @user do |form| %>
    <%= form.label :name, "username:" %>
    <%= form.text_field :name, value: "John"%>
    <%= form.label :email, "type your email here:" %>
    <%= form.email_field :email, placeholder: "example@email.com"%>
    <%= form.label :password, "password:" %>
    <%= form.password_field :password %>
    <%= form.submit "Submit" %>
  <% end %>



